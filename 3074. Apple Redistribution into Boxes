class Solution {
public:
    int minimumBoxes(vector<int>& apple, vector<int>& capacity) {
        sort(capacity.begin(), capacity.end(), greater<int>());
        int n=apple.size();
        int m=capacity.size();
        int sum=0;
        for(int i=0;i<n;i++){
            sum+=apple[i];
        }
        int count =0,j=0;
        while(j<m && sum>0){
            sum-=capacity[j];
            j++;
        }
        return j;
    }
};
