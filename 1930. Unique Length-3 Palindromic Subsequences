class Solution {
public:
    int countPalindromicSubsequence(string s) {
        int n=s.size();
        unordered_set<char> letters;
        for(int i=0;i<n;i++){
                letters.insert(s[i]);
        }
        int result=0;
        for(char ch : letters){
            int left_ind=-1;
            int right_ind=-1;
            for(int i=0;i<n;i++){
                if(s[i]==ch){
                    if(left_ind==-1){
                        left_ind=i;
                    }
                        right_ind=i;
                }
            }
            unordered_set <char> st;
            if (left_ind != -1 && right_ind != -1 && right_ind > left_ind) {
                for(int middle = left_ind + 1; middle < right_ind; middle++){
                    st.insert(s[middle]);
                }
            result+=st.size();
        }
        }
        return result;
    }
};
